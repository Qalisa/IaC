apiVersion: qalisa.github.io/v1alpha1
kind: GithubActionSecretsSync
metadata:
  name: "{{ github_push_secrets_operator__global_gass_name }}"
spec:
  secrets:
    - secretRef: 
        name: "{{ k8s__github_push_secrets_operator__global_secvar_name }}"
        namespace: "{{ k8s__github_push_secrets_operator__namespace }}"
      key: TEST_API_KEY
    - secretRef:
        name: "{{ k8s__github_push_secrets_operator__global_secvar_name }}"
        namespace: "{{ k8s__github_push_secrets_operator__namespace }}"
      key: TEST_DB_PASSWORD
  variables:
    - configMapRef:
        name: "{{ k8s__github_push_secrets_operator__global_secvar_name }}"
        namespace: "{{ k8s__github_push_secrets_operator__namespace }}"
      key: TEST_API_URL
