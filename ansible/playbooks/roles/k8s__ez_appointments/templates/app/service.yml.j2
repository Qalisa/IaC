apiVersion: v1
kind: Service
metadata:
  name: "{{ k8s__ez_appointments__app__name }}"
  namespace: "{{ k8s__ez_appointments__namespace }}"
  labels:
    app: "{{ k8s__ez_appointments__app__name }}"
spec:
  type: LoadBalancer
  selector:
    app: "{{ k8s__ez_appointments__app__name }}"
  ports:
    - name: app
      port: 80
      targetPort: app
      protocol: TCP