apiVersion: v1
kind: Service
metadata:
  name: roundcube
  namespace: docker-mailserver
  labels:
    app: roundcube
  annotations:
    metallb.universe.tf/address-pool: primary
spec:
  type: LoadBalancer
  selector:
    app: roundcube
  ports:
    - name: app
      port: 80
      targetPort: app
      protocol: TCP
